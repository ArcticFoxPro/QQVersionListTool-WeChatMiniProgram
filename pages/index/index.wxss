/* index.wxss */

.page {
    display: flex;
    flex-direction: column;
    min-height: 100vh;
}


.qq-version-item {
    margin-top: 28rpx;
    margin-bottom: 4rpx;
}

.qq-version-list {
    height: 20%;
}

.recycle-view-container {
    flex: 1;
    height: 20%;
    -webkit-overflow-scrolling: touch;

}

.bottom-button {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    gap: 32rpx;
    display: flex;
    justify-content: space-between;
    padding: 32rpx;
    background-color: #f6f6f6;
}

.icon-button {
    display: flex;
    gap: 32rpx;
    align-items: center;
}

.text-button {
    text-align: right;
    flex: 1;
}

.about-header {
    display: flex;
    align-items: center;
    height: 116rpx;
}

.about-title {
    flex: 1;
    text-align: center;
    font-weight: 600;
    font-size: 36rpx;
}

.about-button {
    margin: 32rpx;
}

.about-link {
    display: flex;
    align-items: center;
    justify-content: space-evenly;
    margin-top: 32rpx;
}

.guess-header {
    display: flex;
    align-items: center;
    height: 116rpx;
}

.guess-title {
    flex: 1;
    text-align: center;
    font-weight: 600;
    font-size: 36rpx;
}

.guess-button {
    margin: 32rpx;
}


.demo-section__content {
    margin: 48rpx 32rpx 96rpx 32rpx;
}

.cell-detail-button {
    margin: 32rpx;
}

.header-title {
    font-size: 28px;
    line-height: 36px;
    padding: 8rpx 32rpx 16rpx;
    background-color: #fff;
    font-weight: 600;
    color: rgba(0, 0, 0.9);
}